{
  "name": "@lautr/initium-nuxt-webapp",
  "private": true,
  "scripts": {
    "build": "NITRO_PRESET=vercel-edge nuxt build",
    "dev": "nuxi dev",
    "start": "nuxi start",
    "coverage": "vitest run --coverage",
    "postinstall": "nuxi prepare",
    "test": "vitest run",
    "ci:test": "vitest run",
    "ci:smoke": "pnpm dlx concurrently -k -s first -n \"NUXT,TEST\" -c \"magenta,blue\" \"pnpm dev\" \"pnpm dlx wait-on tcp:3000 && pnpm echo \"pass\" \""
  },
  "dependencies": {
    "@lautr/initium-nuxt-design": "workspace:*"
  },
  "devDependencies": {
    "@nuxt/devtools": "0.3.1",
    "@nuxt/test-utils-edge": "3.3.3-28007403.1fd491f1",
    "@nuxtjs/color-mode": "3.2.0",
    "@nuxtjs/tailwindcss": "6.6.5",
    "@pinia/nuxt": "0.4.7",
    "@vueuse/nuxt": "9.13.0",
    "nuxt": "3.3.3",
    "pinia": "2.0.33",
    "tailwindcss": "3.3.1",
    "vitest": "0.29.8"
  }
}
