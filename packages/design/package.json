{
  "name": "@lautr/initium-nuxt-design",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "storybook dev -p 6006",
    "build": "storybook build",
    "test": "test-storybook --index-json",
    "ci:test": "pnpm dlx concurrently -k -s first -n \"SB,TEST\" -c \"magenta,blue\" \"pnpm build --quiet && pnpm dlx http-server storybook-static --port 6006 --silent\" \"pnpm dlx wait-on http://127.0.0.1:6006 && pnpm test\""
  },
  "dependencies": {
    "vue": "3.3.4"
  },
  "devDependencies": {
    "@nuxt/kit": "3.8.0",
    "@nuxt/schema": "3.8.0",
    "@storybook/addon-essentials": "7.5.3",
    "@storybook/addon-interactions": "7.5.3",
    "@storybook/addon-links": "7.5.3",
    "@storybook/blocks": "7.5.3",
    "@storybook/jest": "0.2.3",
    "@storybook/test-runner": "0.15.2",
    "@storybook/testing-library": "0.2.2",
    "@storybook/vue3": "7.5.3",
    "@storybook/vue3-vite": "7.5.3",
    "@types/node": "18.18.12",
    "@vitejs/plugin-vue": "4.5.0",
    "@vue/tsconfig": "0.4.0",
    "autoprefixer": "10.4.16",
    "postcss": "8.4.31",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "storybook": "7.5.3",
    "storybook-addon-vue-slots": "0.9.25",
    "storybook-dark-mode": "3.0.1",
    "tailwindcss": "3.3.5",
    "unplugin-auto-import": "0.16.7",
    "unplugin-vue-components": "0.25.2",
    "unplugin-vue-macros": "2.7.0",
    "vite": "4.5.0",
    "vue-tsc": "1.8.22"
  }
}
