{
  "name": "@lautr/initium-nuxt-design",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build-storybook": "build-storybook",
    "dev": "start-storybook -p 6006",
    "ci:test": "concurrently -k -s first -n \"SB,TEST\" -c \"magenta,blue\" \"pnpm build-storybook --quiet && npx http-server storybook-static --port 6006 --silent\" \"wait-on tcp:6006 && pnpm test-storybook --ci\""
  },
  "devDependencies": {
    "@babel/core": "7.19.6",
    "@nuxt/kit": "3.0.0-rc.12",
    "@storybook/addon-actions": "6.5.13",
    "@storybook/addon-essentials": "6.5.13",
    "@storybook/addon-links": "6.5.13",
    "@storybook/addon-postcss": "2.0.0",
    "@storybook/builder-vite": "0.2.4",
    "@storybook/test-runner": "0.9.0",
    "@storybook/vue3": "6.5.13",
    "@vitejs/plugin-vue": "3.2.0",
    "autoprefixer": "10.4.13",
    "babel-loader": "8.2.5",
    "playwright": "1.27.1",
    "postcss": "8.4.18",
    "tailwindcss": "3.2.1",
    "typescript": "4.8.4",
    "unplugin-auto-import": "0.11.4",
    "unplugin-vue-components": "0.22.9",
    "vite": "3.2.0",
    "vue": "3.2.41",
    "vue-loader": "17.0.0",
    "vue-tsc": "1.0.9"
  }
}
