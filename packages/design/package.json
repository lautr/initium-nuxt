{
  "name": "@lautr/initium-nuxt-design",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build-storybook": "build-storybook",
    "dev": "start-storybook -p 6006",
    "ci:test": "concurrently -k -s first -n \"SB,TEST\" -c \"magenta,blue\" \"pnpm build-storybook --quiet && npx http-server storybook-static --port 6006 --silent\" \"wait-on tcp:6006 && pnpm test-storybook\""
  },
  "dependencies": {
    "@nuxt/kit": "3.0.0-rc.11",
    "vue": "3.2.39"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/addon-postcss": "2.0.0",
    "@storybook/builder-vite": "^0.2.0",
    "@storybook/test-runner": "0.8.0",
    "@storybook/vue3": "^6.5.9",
    "@vitejs/plugin-vue": "^3.0.0",
    "autoprefixer": "10.4.12",
    "babel-loader": "^8.2.5",
    "postcss": "8.4.14",
    "tailwindcss": "3.1.8",
    "typescript": "^4.6.4",
    "unplugin-auto-import": "0.9.3",
    "unplugin-vue-components": "0.21.1",
    "vite": "^3.0.0",
    "vue-loader": "^17.0.0",
    "vue-tsc": "^0.38.4"
  }
}
