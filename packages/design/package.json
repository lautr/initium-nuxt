{
  "name": "@lautr/initium-nuxt-design",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "storybook dev -p 6006",
    "build": "storybook build",
    "test": "test-storybook --index-json",
    "ci:serve": "pnpm dlx http-server storybook-static --port 6006 . &",
    "ci:test": "pnpm dlx wait-on -l -v http://127.0.0.1:6006 && pnpm test",
    "citest": "pnpm dlx concurrently -k -s first -n \"SB,TEST\" -c \"magenta,blue\" \"pnpm build --quiet && pnpm dlx http-server storybook-static --port 6006 --silent\" \"pnpm dlx wait-on tcp:6006 && pnpm test\""
  },
  "dependencies": {
    "vue": "3.2.47"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "7.0.9",
    "@storybook/addon-interactions": "7.0.9",
    "@storybook/addon-links": "7.0.9",
    "@storybook/blocks": "7.0.9",
    "@storybook/jest": "0.1.0",
    "@storybook/test-runner": "0.10.0",
    "@storybook/testing-library": "0.1.0",
    "@storybook/vue3": "7.0.9",
    "@storybook/vue3-vite": "7.0.9",
    "@types/node": "18.16.6",
    "@vitejs/plugin-vue": "4.2.1",
    "@vue/tsconfig": "0.3.2",
    "autoprefixer": "10.4.14",
    "postcss": "8.4.23",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "storybook": "7.0.9",
    "storybook-dark-mode": "3.0.0",
    "tailwindcss": "3.3.2",
    "unplugin-auto-import": "0.15.3",
    "unplugin-vue-components": "0.24.1",
    "unplugin-vue-macros": "2.1.3",
    "vite": "4.3.5",
    "vue-tsc": "1.6.4"
  }
}
